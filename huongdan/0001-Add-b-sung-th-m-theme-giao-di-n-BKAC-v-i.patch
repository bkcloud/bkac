+	text-align: left;
+	z-index: 20;
+	-moz-border-radius: 0 10px 10px 10px;
+	filter: progid:DXImageTransform.Microsoft.Alpha(opacity=87);
+	-moz-opacity: .87;
+	-khtml-opacity: .87;
+	opacity: .87;
+}
+div.niceTitle h1
+{
+  background: #4d77a7;
+  border-bottom: 1px dotted #FFFFFF;
+	font-weight: bold;
+	font-size: 13px;
+	font-family: "Trebuchet MS", sans-serif;
+	margin: 3px;
+  padding-top: 1px;
+  padding-bottom: 1px;
+  padding-left: 3px;
+  text-align: left;
+	left: 0;
+	top: 0;
