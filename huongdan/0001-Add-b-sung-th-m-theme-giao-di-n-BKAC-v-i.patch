+/* DOM Tooltip CSS definitions */
+div.niceTitle
+{
+	background-color: #333333;
+	color: #FFFFFF;
+	font-weight: bold;
+	font-size: 13px;
+	font-family: "Trebuchet MS", sans-serif;
+	width: 220px;
+	left: 0;
+	top: 0;
